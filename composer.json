{
    "name": "asseco-voice/laravel-custom-fields",
    "description": "Laravel support for custom fields",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "mockery/mockery": "^1.4.4",
        "fakerphp/faker": "^1.9.1",
        "orchestra/testbench": "^9.0",
        "doctrine/dbal": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Asseco\\CustomFields\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/ibnu-ja/laravel-blueprint-audit"
        },
        {
            "type": "vcs",
            "url":  "https://github.com/ibnu-ja/laravel-common"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "Asseco\\CustomFields\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asseco\\CustomFields\\CustomFieldsServiceProvider"
            ]
        }
    }
}
